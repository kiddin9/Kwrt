/* watchdog */
&wdt {
        status = "okay";
};

&sfc {
	status = "okay";
	max-freq = <100000000>;
	#address-cells = <1>;
	#size-cells = <0>;
	pinctrl-names = "default";
	pinctrl-0 = <&fspim0_pins>;

	spi_flash: spi-flash@0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "jedec,spi-nor";
		reg = <0x0>;
		spi-max-frequency = <100000000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <4>;
		status = "okay";

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot@0 {
				label = "uboot";
				reg = <0x0 0x200000>;
			};

			dtb@200000 {
				label = "dtb";
				reg = <0x200000 0x40000>;
			};

			firmware@240000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x240000 0xdc0000>;
			};

		};
	};
};

